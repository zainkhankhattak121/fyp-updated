{
  "expo": {
    "name": "Deepfake",
    "slug": "Deepfake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "deepfakeapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepfakeapp.deepfake",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.deepfakeapp.deepfake",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to audio files"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your audio files"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7b4fe032-8710-471b-8e0d-c9ad96c85ca3"
      }
    }
  }
}
